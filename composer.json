{
  "name": "vlcice/piglatin",
  "description": "Pig Latin - an English game of alterations",
  "keywords": [
    "game",
    "piglatin",
    "english"
  ],
  "homepage": "https://github.com/vlcice/piglatin",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Martina Volfova",
      "email": "vlcice.martina@gmail.com"
    }
  ],
  "repositories": {
    "oxyshop": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Vlcice\\PigLatin\\": "src"
    }
  },
  "scripts": {
    "build": [
      "@composer install --no-progress --no-interaction --no-suggest"
    ],
    "lint": "parallel-lint src --exclude vendor .",
    "cs-fix": "php-cs-fixer fix -v",
    "phpstan": "phpstan analyse -c phpstan.neon -l 2 src"
  },
  "require": {
    "twig/twig": "^2.5",
    "symfony/var-dumper": "^4.1",
    "components/bootstrap": "^4.1"
  }
}
